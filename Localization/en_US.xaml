<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="LOCStartButtonLauncherSelectButton">
        Select a button to launch Playnite fullscreen mode:
    </system:String>
    <system:String x:Key="LOCStartButtonLauncherGuideWarning">
        Note: The Guide button may not always be detected. Please test if it works with your controller.
    </system:String>
    <system:String x:Key="LOCStartButtonLauncherFocusOption">
        Focus Playnite before fullscreen
    </system:String>
    <system:String x:Key="LOCStartButtonLauncherFocusOptionHint">
        If enabled, Playnite Desktop will be brought to front before switching to fullscreen.
    </system:String>
    <system:String x:Key="LOCStartButtonLauncherLogTitle">
        Button input test:
    </system:String>
    <system:String x:Key="LOCStartButtonLauncherLogButtonPressed">
        Button pressed:
    </system:String>

</ResourceDictionary>
