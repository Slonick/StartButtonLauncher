<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="LOCStartButtonLauncherSelectButton">
        Wählen Sie eine Taste, um Playnite im Vollbildmodus zu starten:
    </system:String>
    <system:String x:Key="LOCStartButtonLauncherGuideWarning">
        Hinweis: Die Guide-Taste wird möglicherweise nicht immer erkannt. Bitte testen Sie, ob sie mit Ihrem Controller funktioniert.
    </system:String>
    <system:String x:Key="LOCStartButtonLauncherFocusOption">
        Playnite vor dem Vollbildmodus fokussieren
    </system:String>
    <system:String x:Key="LOCStartButtonLauncherFocusOptionHint">
        Wenn aktiviert, wird Playnite Desktop in den Vordergrund gebracht, bevor der Vollbildmodus aktiviert wird.
    </system:String>
    <system:String x:Key="LOCStartButtonLauncherLogTitle">
        Tasten-Eingabetest:
    </system:String>
    <system:String x:Key="LOCStartButtonLauncherLogButtonPressed">
        Gedrückte Taste:
    </system:String>

</ResourceDictionary>
